   "source": [
    "# Write a Python program to reverse a string.\n",
    "\n",
    "# Sample String : \"1234abcd\"\n",
    "\n",
    "# Expected Output : \"dcba4321\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "c93a5cdf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Please enter a string: Edyoda\n",
      "adoydE\n"
     ]
    }
   ],
   "source": [
    "value = input(\"Please enter a string: \")\n",
    "\n",
    "def rev_string(x):\n",
    "    return x[::-1]\n",
    "\n",
    "y = rev_string(value)\n",
    "\n",
    "print(y)"
   ]
